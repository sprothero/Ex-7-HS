
# ////////////////////////////////////////////////////////////////////////////////
# ///                     Declarations and Initialization                      ///
# ////////////////////////////////////////////////////////////////////////////////

<AppScreen>:
    name: 'new'

    FloatLayout:
        size_hint: None, None


# ////////////////////////////////////////////////////////////////////////////////
# ///                                 Widgets                                  ///
# ////////////////////////////////////////////////////////////////////////////////

    DPEAButton: # button that changes screens
        id: changeScreenBack
        text: "Back"
        size: 1175, 100
        x: root.width * 0.2
        y: root.height * 0.85
        color: 1.0, 0.29, 0.29, 1
        on_press: root.screen_transition_back()